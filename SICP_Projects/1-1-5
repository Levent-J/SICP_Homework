#lang racket
(define size 2)
(* 5 size)
(define pi 3.1415926)
(define radius 10)
(* pi (* radius radius ))
(define cir(* 2 pi radius))
cir

(define (square x) (* x x))
(square 2)

(define (sumofsquare x y) (+ (square x) (square y)) )
(sumofsquare 3 4)

(define (f a)
  (sumofsquare (+ a 1) (* a 2))
  )
(f 5)